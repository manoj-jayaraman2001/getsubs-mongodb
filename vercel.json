{
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/subscribers/(.*)",
        "dest": "src/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "src/app.js"
      }
    ],
    "env": {
      "MONGODB_URI": "@mongo-uri"
    },
    "static": {
      "public": "public"
    },
    "build": {
      "env": {
        "BUILD_ENV": "production"
      },
      "use": "@vercel/node",
      "config": {
        "zeroConfig": true
      },
      "command": "node src/createDatabase.js && npm run build"
    }
  }
  