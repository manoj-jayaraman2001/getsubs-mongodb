{
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/subscribers/(.*)",
        "dest": "app.js"
      },
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "env": {
      "MONGODB_URI": "@mongo-uri"
    }
  }
  